{
  "name": "bakery.js",
  "version": "0.0.1",
  "description": "A RESTful API framework based on Bun",
  "repository": "https://github.com/lanezhao/bakery.js",
  "author": "Lane <lane@wiseinfo.cc>",
  "type": "module",
  "main": "index.js",
  "imports": {
    "#bakery": "./index.js",
    "#lib": "./lib/index.js",
    "#lib/*": "./lib/*.js",
    "#modules": "./modules/index.js",
    "#modules/*": "./modules/*/index.js",
    "#utils": "./utils/index.js",
    "#utils/*": "./utils/*.js"
  },
  "scripts": {
    "test": "bun test --coverage",
    "test:watch": "bun test --watch --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "bun-types": "latest",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^9.0.11",
    "typescript": ">=4.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}