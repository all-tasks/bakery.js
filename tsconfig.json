{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "types": ["bun-types"],
    "useDefineForClassFields": true,
    "baseUrl": ".",
    "paths": {
      "#lib": ["lib"],
      "#lib/*": ["lib/*"],
      "#modules": ["modules"],
      "#modules/*": ["modules/*"]
    }
  },
  "references": []
}
